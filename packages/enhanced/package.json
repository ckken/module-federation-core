{
  "name": "@module-federation/enhanced",
  "version": "0.6.10",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "repository": "https://github.com/module-federation/core/tree/main/packages/enhanced",
  "files": [
    "dist/",
    "README.md"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "webpack": "^5.0.0",
    "typescript": "^4.9.0 || ^5.0.0",
    "vue-tsc": ">=1.0.24"
  },
  "peerDependenciesMeta": {
    "webpack": {
      "optional": true
    },
    "typescript": {
      "optional": true
    },
    "vue-tsc": {
      "optional": true
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.mjs",
      "require": "./dist/webpack.js"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "import": "./dist/rspack.mjs",
      "require": "./dist/rspack.js"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.mjs",
      "require": "./dist/runtime.js"
    },
    "./prefetch": {
      "types": "./dist/prefetch.d.ts",
      "import": "./dist/prefetch.mjs",
      "require": "./dist/prefetch.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "webpack": [
        "./dist/webpack.d.ts"
      ],
      "rspack": [
        "./dist/rspack.d.ts"
      ],
      "runtime": [
        "./dist/runtime.d.ts"
      ],
      "prefetch": [
        "./dist/prefetch.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@module-federation/webpack-bundler-runtime": "workspace:*",
    "@types/btoa": "^1.2.5",
    "enhanced-resolve": "^5.0.0",
    "tsup": "^8.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@module-federation/sdk": "workspace:*",
    "@module-federation/runtime-tools": "workspace:*",
    "@module-federation/manifest": "workspace:*",
    "@module-federation/managers": "workspace:*",
    "@module-federation/dts-plugin": "workspace:*",
    "@module-federation/rspack": "workspace:*",
    "@module-federation/bridge-react-webpack-plugin": "workspace:*",
    "@module-federation/data-prefetch": "workspace:*",
    "upath": "2.0.1",
    "btoa": "^1.2.1"
  }
}
